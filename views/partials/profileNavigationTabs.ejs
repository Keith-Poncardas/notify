  <% const links = [
  {
    title: 'Posts',
    path: `/${viewUser.username}`
  },
  {
    title: 'Liked',
    path: `/${viewUser.username}/likes`
  },
  {
    title: 'Followers',
    path: `/${viewUser.username}/followers`
  }
  ]; %>

  <!-- Profile Navigation Tabs -->
  <div class="border-top rounded-bottom-3 pt-1 px-3">
    <ul class="nav nav-tabs border-0">
      <% links.forEach(link => { %>
      <li class="nav-item"></li>
      <a class="nav-link disabled <%= path === link.path || path === `${link.path}/page/${currentPage}` ? 'rounded-top active fw-semibold' : 'text-secondary hover-primary'  %>" href="<%= link.path %>" style="transition: all 0.2s ease-in-out;">
        <%= link.title %>
      </a>
      </li>
      <% }) %>
    </ul>
  </div>