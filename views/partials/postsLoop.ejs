<% if (posts.length === 0) { %>
<%- include('../partials/emptyState', { icon: 'far fa-folder-open', text: 'No Posts Available', subtext: '', bold: false }); %>
<% } else { %>
<% posts.forEach((post) => { %>
<%- include('../partials/postCard', { post, isDisabled: false, isExpand: false  }) %>
<% }) %>
<% } %>